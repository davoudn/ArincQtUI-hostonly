import serial

# Open the serial port
ser = serial.Serial(
    port='/dev/ttyS1',     # or 'COM3' on Windows
    baudrate=115200,
    bytesize=serial.EIGHTBITS,
    parity=serial.PARITY_NONE,
    stopbits=serial.STOPBITS_ONE,
    timeout=1                # seconds
)

print("Listening on", ser.port)

a = []
for i in range(10):
    a.append(i)

ser.write(a)

# Read loop
while 1==1:
#input()
#ser.write(a)
    data = ser.read(10)     # read up to 256 bytes
    print ("\n")
    if data:
        for x in data:
            print("Received:", x)
